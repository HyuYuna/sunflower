body,button		{margin:0; padding:0; font-family:"sans-serif";}

#divHead		{ border-bottom:1px solid #ffffff; }
#divLeft		{ display:inline; float:left; width:180px; border-right:1px solid #ffffff; }
#divMain		{ display:inline; float:left; width:700px; }
#divRight		{ position:absolute; left:800px; top:100px; width:150px; border-right:1px solid #ffffff; margin:10px 0 0 10px;}
#divTail		{ clear:both; border-top:1px solid #aaaaaa; padding-left:20px;}

#contain		{ margin:10px 0 10px 10px; }

.table100 			{ width:100%; border-collapse:collapse; border:1px gray solid; }
.table100 thead 	{ background-color:#dddddd; }
.table100 th b {
    border-bottom:1px solid orange;
	padding-bottom:1px;
}

.ui-datepicker select.ui-datepicker-month,.ui-datepicker select.ui-datepicker-year{width:40%}

