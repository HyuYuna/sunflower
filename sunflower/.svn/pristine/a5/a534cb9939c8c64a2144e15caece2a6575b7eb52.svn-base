/* 코드목록 */
SELECT
	A.CL_CD,
    A.CL_CD_NM,
    B.CD_ID,
    B.CD_ID_NM
FROM TC_CMMN_CD_CL A, TC_CMMN_CD_CTGRY B
WHERE A.CL_CD = B.CL_CD
AND A.USE_AT = 'Y' AND B.USE_AT='Y'
ORDER BY A.CL_CD_NM, CD_ID_NM;

/* 코드정의서 조회 */
SELECT
	A.CL_CD,
    A.CL_CD_NM,
    B.CD_ID,
    B.CD_ID_NM,
    C.CD,
    C.CD_NM,
    C.CD_DC
FROM TC_CMMN_CD_CL A, TC_CMMN_CD_CTGRY B, TC_CMMN_CD_DETAIL C
WHERE A.CL_CD = B.CL_CD
AND B.CD_ID = C.CD_ID
AND A.USE_AT = 'Y' AND B.USE_AT='Y' AND C.USE_AT ='Y'
AND A.CL_CD <> 'EFC'
ORDER BY A.CL_CD_NM, CD_ID_NM , CD_NM;