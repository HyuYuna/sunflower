/*-------------------------------------
  레이아웃
--------------------------------------*/
#wrap {width:100%;}
#header{position:relative;width:100%;min-width:1200px;height:90px; border-bottom:2px solid #363636;}
#container{display:block;width:100%;}
#leftmenu{width:220px; float:left;}
#main{float:none;min-height:748px;margin-left:220px;padding:0 0 30px 0px;box-sizing:border-box;border-left:1px solid #dbdbdb;}
#contents{float:none;min-height:820px;margin-left:220px;padding:30px 0 30px 40px;box-sizing:border-box;border-left:1px solid #dbdbdb;}
#footer{width:100%;min-width:1200px;padding:20px 20px 40px; box-sizing:border-box;background:#ececec;clear:both;position:relative;bottom:0;}

.ui-tabs .ui-tabs-panel { padding:0 }
.ui-widget-content { background:#ffffff }
