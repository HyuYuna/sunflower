@charset "UTF-8";
   dl, dt, dd, ul{margin:0; padding:0;}
	body {position:relative;padding:40px;background: #FFFFFF no-repeat center fixed;box-sizing:border-box; font-size:13px;}
	table{margin-top:10px; margin-bottom:10px; padding:0;border-collapse:collapse;}
	textarea,input{border:1px solid #dbdbdb;vertical-align:middle;}
	textarea {white-space: pre-wrap;}
	h1{margin-top:0;margin-bottom:30px;text-align:center}
	h4{height:20px;margin:20px 0 5px;padding-left:18px;background:url('/static/bos/image/bul_circle1.png') left -1px no-repeat;font-size:14px;font-weight:bold;}
	li{list-style:none;}
	
	
    .sr-only{display:none;}
	.table01{width:100%;border:2px solid #000;}
	.table01 th{padding:8px 10px;border:1px solid #bbb;text-align:center;}
	.table01 td{padding:8px 10px;border:1px solid #bbb;text-align:left;}
	.table01 tfoot td{border-top:2px solid #000;font-size:17px;text-align:center;line-height:26px;}

	.table02{width:100%;border:2px solid #000 !important;}
	.table02 thead th{padding:5px 10px;border:1px solid #bbb; border-bottom:1px solid #000;background:#e9e9e9;color:#3378b3;}
	.table02 tbody th{padding:5px 10px;border:1px solid #bbb;background:#e9e9e9;text-align:center;}
	.table02 tbody td{padding:5px 10px;border:1px solid #bbb;text-align:left;}
	.table02 tfoot td {padding:10px; border:2px solid #000;line-height:20px;}
	.table02 tfoot td > p{font-weight:bold; font-size:15px;text-align:center; line-height:30px;}
	.table02 tfoot td > dl{width:75%;margin:20px auto;}
	.table02 tfoot td > dl > dd > span{float:left;width:70px; margin-right:10px;}
	.table02 tfoot td > dl > dd{letter-spacing:-0.5px;}
	.table02 input{vertical-align:middle;}
	
	.table02 tfoot td > dl > dd > span.tit{float:left;}
	.table02 tfoot td > dl > dd > strong{float:none;display:block;max-width:450px;margin-left:80px;font-weight:normal;}
	.table02 tfoot td > dl > dd:nth-child(3){clear:both;}


	.table02 .tb{width:100%; border:1px;}
	.table02 .tb > tbody > tr > th{padding:5px; border:0px; border-right:1px solid #bbb;}
	.table02 .tb > tbody > tr > td{padding:5px; border:0px; border-right:1px solid #bbb;}
	.table02 .tb > tbody > tr > td.lasttd{padding:5px; border:0px;}
	
	.table02 .tb > tfoot > tr > th{border-right:1px solid #bbb;}
	.table02 .tb > tfoot > tr > td{border-right:1px solid #bbb;}

	th.title {font-size:25px;font-weight:bold;border-right:1px solid #000;}
	.border_top{border-top:2px solid #000;}
	.border_top_none{border-top:none;}
	.bg_gray{background:#f1f1f1;}
	.print_date{margin-top:10px;text-align:right}
	.print_date2{margin-top:30px; text-align:right !important;}
	.print_date2 > p{font-weight:bold;font-size:16px;}
	.mt30{margin-top:30px;}
	.btnGroup{position:absolute; left:5px; top:5px;}

	.txt{display:block;margin:5px 0;font-size:12px;}
	.txt_list1{margin-bottom:10px;color:#888;font-size:11px;}
	.txt_list1 dt{margin-top:5px;padding-left:8px;background:url('/image/bul_bar1.gif') left 5px no-repeat;font-weight:bold;}
	.txt_list1 dd{margin-left:9px;}

	.add{margin:10px 0;}

	.tit_line{padding:10px;border-top:2px solid #000;border-bottom:2px solid #000;}

	.ac{text-align:center !important;}
	.c_both{clear:both !important;}
