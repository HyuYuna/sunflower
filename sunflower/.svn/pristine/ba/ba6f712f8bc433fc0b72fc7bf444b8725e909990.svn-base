	.div_td_title {padding:5px 10px;border-left:2px solid #000;border-right:2px solid #000;text-align:left;background:#e9e9e9;font-weight: bold;}
	.div_td_content {padding:5px 10px;border:2px solid #000;border-top:1px solid #bbb;text-align:left;overflow:hidden;word-wrap:break-word;}
	.div_foot_content {border:2px solid #000; margin-top:10px;}
	.div_foot_content > p{font-weight:bold; font-size:15px;text-align:center; line-height:30px;}
	.div_foot_content > dl{width:75%;margin:20px auto;}
	.div_foot_content > dl > dd > span{float:left;width:70px; margin-right:10px;}
	.div_foot_content > dl > dd{letter-spacing:-0.5px;}	
	.div_foot_content > dl > dd > span.tit{float:left;}
	.div_foot_content > dl > dd > strong{float:none;display:block;max-width:450px;margin-left:80px;font-weight:normal;}
