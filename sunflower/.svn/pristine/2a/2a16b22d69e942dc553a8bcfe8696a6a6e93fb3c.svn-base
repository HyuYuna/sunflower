.main_cont:after{display:block;content:"";clear:both;}
.main_cont > .cont01{float:left;width:46%;height:200px;min-height:150px;margin:0 22px 22px 0;padding:18px;box-sizing:border-box;box-shadow:#f0f0f0 1px 2px 2px;border-radius:5px;border:1px solid #cecece;}
.main_cont > .cont02{float:left;width:46%;min-height:150px;margin-bottom:22px;padding:18px;box-sizing:border-box;box-shadow:#f0f0f0 1px 2px 2px;border-radius:5px;border:1px solid #cecece;}
.main_cont > .cont01 > h2, .main_cont > .cont02 > h2{position:relative;margin-bottom:12px;padding:7px 15px;background:#fee000;box-shadow:#f5f5f5 0 2px 0;border-radius:20px;font-size:14px;}
.main_cont > .cont01 > h2 > span, .main_cont > .cont02 > h2 > span{position:absolute; right:15px;top:13px;}
.main_cont > .cont01 > ul, .main_cont > .cont02 > ul{margin:0 5px;}
.main_cont > .cont01 > ul > li, .main_cont > .cont02 > ul > li {position:relative;padding-left:8px;background:url('../image/bul_arrow3.png') left 9px no-repeat;line-height:22px;}
.main_cont > .cont01 > ul > li > span, .main_cont > .cont02 > ul > li > span {margin-right:5px;}
.main_cont > .cont01 > ul > li > strong, .main_cont > .cont02 > ul > li > strong {font-weight:normal; color:#747474; position:absolute; top:0; right:0;}


