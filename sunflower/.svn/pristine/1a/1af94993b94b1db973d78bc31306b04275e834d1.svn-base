log4j.rootLogger=WARN,A1

# A1 is set to be a ConsoleAppender.
log4j.appender.A1=org.apache.log4j.ConsoleAppender
log4j.appender.A1.layout=org.apache.log4j.PatternLayout
log4j.appender.A1.layout.ConversionPattern=%d{ISO8601} [%t] %-5p %c %x - %m%n

og4j.additivity.org.springframework.web=false
og4j.additivity.site=false

log4j.logger.org.springframework.web=DEBUG, A1
log4j.logger.site=DEBUG, A1